{
  "name": "scholartrack-client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cleanup": "pkill -f 'vite' || true",
    "dev": "npm run cleanup && vite",
    "build": "vue-tsc --noEmit -p tsconfig.typecheck.json && vite build --mode production",
    "build:vite": "vite build --mode production",
    "build:type-check": "vue-tsc && vite build",
    "preview": "vite preview",
    "type": "vue-tsc --noEmit -p tsconfig.typecheck.json",
    "type-check": "vue-tsc --noEmit",
    "t": "vue-tsc --noEmit -p tsconfig.typecheck.json && cd ../server && npx tsc --noEmit && cd ../client",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage --reporter=html",
    "test:coverage:lcov": "vitest run --coverage --reporter=lcov",
    "test:watch": "vitest --watch",
    "test:related": "vitest --run --reporter=verbose",
    "test:unit": "vitest run --dir=src/components",
    "test:stores": "vitest run --dir=src/stores",
    "test:utils": "vitest run --dir=src/utils",
    "test:integration": "vitest run --dir=src --run",
    "test:debug": "vitest --run --reporter=verbose --no-coverage",
    "test:ci": "vitest run --coverage --reporter=json --reporter=lcov",
    "test:components": "vitest run --config=vitest.component.config.ts",
    "test:components:watch": "vitest --config=vitest.component.config.ts --watch",
    "test:components:coverage": "vitest run --config=vitest.component.config.ts --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "axios": "^1.6.0",
    "idb": "^8.0.0",
    "moment": "^2.29.4",
    "pinia": "^2.1.7",
    "vue": "^3.5.19",
    "vue-router": "^4.2.5",
    "vuetify": "^3.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}
