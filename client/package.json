  },
  "dependencies": {
    "vue": "^3.3.8",
    "vuetify": "^3.9.0",
    "@mdi/font": "^7.3.67",
    "idb": "^8.0.0",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.22",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "@vue/eslint-config-typescript": "^12.0.0"
